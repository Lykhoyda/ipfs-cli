{
  "name": "ipfs-cli",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Anton Lykhoyda lykhoyda@gmail.com",
  "bin": {
    "ipfs-cli": "./bin/run"
  },
  "homepage": "https://github.com/lykhoyda/ipfs-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "lykhoyda/ipfs-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@truffle/hdwallet-provider": "^2.0.8",
    "@types/jest": "^27.5.0",
    "dotenv": "^16.0.0",
    "ipfs-http-client": "^56.0.3",
    "mock-fs": "^5.1.2",
    "truffle-hdwallet-provider": "^1.0.17",
    "ts-jest": "^28.0.1",
    "web3": "^1.7.3",
    "web3-eth-contract": "^1.7.3",
    "web3-utils": "^1.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/node": "^17.0.31",
    "babel-jest": "^28.0.3",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.5.0",
    "globby": "^11",
    "jest": "^28.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "2.6.2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "oclif": {
    "bin": "ipfs-cli",
    "dirname": "ipfs-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "jest --verbose --rootDir=src",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "bugs": "https://github.com/lykhoyda/ipfs-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
